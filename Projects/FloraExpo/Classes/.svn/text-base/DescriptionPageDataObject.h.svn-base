#import <Foundation/Foundation.h>

//儲存詳細資訊之資料物件
@interface DescriptionPageDataObject : NSObject {
	//該詳細資訊之key
	NSString *key;
	//該詳細資訊
	NSString *title;
	//若有圖片,其圖片名稱
	NSString *picName;
	//資料內容,儲放物件應為 DescriptionObject
	NSMutableArray *contentArray;
}
@property(nonatomic,retain)NSString *key,*title,*picName;
@property(nonatomic,retain)NSMutableArray *contentArray;
@end
