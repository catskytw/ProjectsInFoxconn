#import <Foundation/Foundation.h>


@interface CommonItem : NSObject {
	//該item欲顯示於畫面之名稱
	NSString *itemName;
	//item之代表號
	NSString *itemKey;
	//該item之圖片,若無則不必使用
	NSString *itemPic;
}
@property(nonatomic,retain)NSString *itemName;
@property(nonatomic,retain)NSString *itemKey;
@property(nonatomic,retain)NSString *itemPic;
-(id)initWithItemKey:(NSString*)key withItemName:(NSString*)name;
-(id)initWithItemKey:(NSString*)key withItemName:(NSString*)name withItemPic:(NSString*)picName;

@end
