#import <Foundation/Foundation.h>


@interface SignInObject : NSObject {
    //使用者資料
    @public
    BOOL isLogin;
    BOOL isMember;
    NSString *sessionId;
    NSString *memberId;
    NSString *account;
    NSString *password;
    NSString *name;
    NSString *workIncome;
    NSString *birthday;
    NSString *sex;
    NSString *phone;
    NSString *storeName;
    NSString *cityName;
    NSString *postCode;
    NSString *sectionName;
    NSString *provinceName;
    NSString *code;
    NSString *workJob;
    NSString *marryStatus;
    NSString *habit;
    NSString *address;
    NSString *email;
    NSString *storeId;
    NSString *mobile;
}
@property(nonatomic)BOOL isLogin,isMember;
@property(retain,nonatomic)NSString *name,*sessionId,*workIncome,*birthday,*sex,*storeName,*cityName,*postCode,*sectionName,*provinceName,*code,*workJob,*marryStatus,*habit,*address,*email,*storeId,*mobile,*account,*password,*phone,*memberId;
+(SignInObject*)share;
+(void)SignInRelease;
@end
